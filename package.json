{
  "name": "api-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check \"src/**/*.ts\" \"src/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/node": "^17.0.17",
    "axios": "^0.26.0",
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "module-alias": "^2.2.2"
  }
}
